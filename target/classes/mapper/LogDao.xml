<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zxc.dao.LogDao">
    <select id="queryAll" resultType="Logging_event">
        SELECT * FROM Logging_event
    </select>
     <insert id="addLog" parameterType="LogEntity" >
        INSERT INTO LogEntity(userId,module,method,response_date,ip,data,commit)
        VALUES (#{userId},#{module},#{method},#{response_date},#{ip},#{data},#{commit})
    </insert>
</mapper>